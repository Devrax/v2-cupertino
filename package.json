{
  "name": "v2-cupertino",
  "version": "1.2.4",
  "author": {
    "name": "Rafael Mejia"
  },
  "bugs": {
    "url": "https://github.com/Devrax/v2-cupertino/issues"
  },
  "license": "MIT",
  "main": "dist/v2-cupertino.umd.js",
  "module": "dist/v2-cupertino.esm.js",
  "unpkg": "dist/v2-cupertino.min.js",
  "browser": {
    "./sfc": "src/components/v2-cupertino.vue"
  },
  "scripts": {
    "build": "npm run build:umd && npm run build:es && npm run build:unpkg",
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/v2-cupertino.umd.js",
    "build:es": "rollup --config rollup.config.js --format es --file dist/v2-cupertino.esm.js",
    "build:unpkg": "rollup --config rollup.config.js --format iife --file dist/v2-cupertino.min.js"
  },
  "dependencies": {
    "cupertino-pane": "^1.2.4"
  },
  "peerDependencies": {
    "vue": "^2.6.11",
    "cupertino-pane": "^1.2.4"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "core-js": "^3.6.5",
    "rollup": "^1.32.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-vue": "^5.1.9",
    "typescript": "~3.9.3",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "description": "A Vue 2 Wrapper for Cupertino Pane Library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Devrax/v2-cupertino"
  }
}
